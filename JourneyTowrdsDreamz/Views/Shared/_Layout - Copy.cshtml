<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
   
    <title>Angular QuickStart</title>
    <base href="/src/">
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="styles.css">
    <!-- Polyfill(s) for older browsers -->
    <script src="/node_modules/core-js/client/shim.min.js"></script>
    <script src="/node_modules/zone.js/dist/zone.js"></script>
    <script src="/node_modules/systemjs/dist/system.src.js"></script>
    <script src="systemjs.config.js"></script>
    <script>
      System.import('main.js').catch(function(err){ console.error(err); });
    </script>
    



    <link href="~/css/tooplate_style.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="~/css/coda-slider.css" type="text/css" charset="utf-8" />
    <script src="~/js/jquery-1.2.6.js" type="text/javascript"></script>
    <script src="~/js/jquery.scrollTo-1.3.3.js" type="text/javascript"></script>
    <script src="~/js/jquery.localscroll-1.2.5.js" type="text/javascript" charset="utf-8"></script>
    <script src="~/js/jquery.serialScroll-1.2.1.js" type="text/javascript" charset="utf-8"></script>
    <script src="~/js/coda-slider.js" type="text/javascript" charset="utf-8"></script>
    <script src="~/js/jquery.easing.1.3.js" type="text/javascript" charset="utf-8"></script>





</head>
<body>
    @*<div class="">
        Journey Towards Dreamz
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
       
    </div>
    <div>

    </div>*@


    <div id="slider">
        <div id="tooplate_wrapper">

            <div id="header">

                <h1><a href="#"><img src="images/tooplate_logo.png" alt="Sound Master" /></a></h1>

            </div>

            <div id="tooplate_main">

                <div id="menu">
                    <ul class="navigation">
                        <li><a href="#home" class="selected">Home<span class="ui_icon home"></span></a></li>
                        <li><a href="/src/addUser">User<span class="ui_icon aboutus"></span></a></li>
                        <li><a href="#services">Consumer<span class="ui_icon services"></span></a></li>
                        <li><a href="#gallery">Login<span class="ui_icon gallery"></span></a></li>
                        <li><a href="#contact">Admin<span class="ui_icon contactus"></span></a></li>
                        <li><a href="#contact">Contact<span class="ui_icon contactus"></span></a></li>
                    </ul>
                </div>

                <div id="content">
                    <div class="scroll">
                        <div class="scrollContainer">

                            @RenderBody()

                    
                        </div>
                    </div> <!-- end of scroll -->

                </div>
            </div> <!-- end of content -->

        </div> <!-- end of wrapper -->
        <div id="footer">

            <div id="social_box">
                <a href="#"><img src="~/images/facebook.png" alt="facebook" /></a>
                <a href="#"><img src="~/images/flickr.png" alt="flickr" /></a>
                <a href="#"><img src="~/images/myspace.png" alt="myspace" /></a>
                <a href="#"><img src="~/images/twitter.png" alt="twitter" /></a>
                <a href="#"><img src="~/images/youtube.png" alt="youtube" /></a>
            </div>

            <div id="footer_left">
                Copyright © 2048 <a href="#">Company Name</a><br />
                <!--
                <a href="http://www.tooplate.com/view/2022-sound-master">Sound Master</a>
                by <a rel="nofollow" href="http://www.tooplate.com">Templates</a>
                -->
            </div>

            <div class="cleaner"></div>
        </div>
    </div>




    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
